<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="CustomButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/bgr_green_button</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">@dimen/choose_number_keyboard_key_text</item>
        <item name="android:fontFamily">@font/roboto_medium</item>
    </style>
    <style name="titleText">
        <item name="android:textSize">@dimen/choose_number_phone_status_height</item>
    </style>
    <style name="AppTheme.Splash" parent="@style/Theme.AppCompat.NoActionBar">
    </style>


</resources>
